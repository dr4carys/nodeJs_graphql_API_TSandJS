        "apollo-engine": "^1.1.2",
        "apollo-server": "^3.5.0",
        "apollo-server-express": "^3.5.0",
        "aws-sdk": "^2.1033.0",
        "axios": "^0.24.0",
        "babel-preset-env": "^1.7.0",
        "body-parser": "^1.19.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
