        "jwk-to-pem": "^2.0.5",
        "mongoose": "^6.0.13",
        "mongoose-timestamp": "^0.6.0",
        "nodemon": "^2.0.15",
        "request": "^2.88.2",
        "apollo-server-lambda": "3.4.0",
